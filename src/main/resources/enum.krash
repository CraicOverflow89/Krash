// Create Enum
enum Direction {
    EAST, NORTH, SOUTH, WEST
}

// Print Enum
echo(Direction)

// Enum Value
n = (Direction.NORTH)
echo(n)

// Equality Comparisons
echo(n == n)
echo(n == Direction.NORTH)
echo(n == Direction.SOUTH)

// Value Of
echo(Direction.valueOf("EAST"))
echo(Direction.valueOf("INVALID"))

// Values
echo(Direction.values)