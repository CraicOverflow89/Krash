// Create Array
data = ["Kotlin"]

// Add Data (add method)
data.add("Python")

// Add Data (short syntax)
data[2] = "Java"

// Print Array
echo(data)

// Print Size
echo("size = " + @data.size())

// Print Each
data.each(echo)

// Join Elements
echo(data.join("|", "<", ">"))

// Map Elements
echo(data.map(fun() {
    return it + "!"
}))

// Filter Elements
echo(data.filter(fun() {
    return it[0] == "K"
}))

// Reject Elements
echo(data.reject(fun() {
    return it[0] == "J"
}))