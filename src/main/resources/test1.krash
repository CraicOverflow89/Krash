// Basic Literals
person = {name: "James", age: 30, isMale: true, lang: ["Kotlin", "Python"]}
echo(person)

/*
 * Says Hello
 *
 * @param name
 * @returns void
 */
hello = fun(name = "Somebody") {
    echo("Hello " + name)
}

// Invoke Callable
hello(person["name"])

// IIFE
fun(){echo("IIFE")}()

// Simple Callable
simple = fun() = echo("simple callable")
simple()